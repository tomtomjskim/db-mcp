{
  "name": "db-mcp-server",
  "version": "1.0.0",
  "description": "Model Context Protocol server for database operations with AI agents",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "database",
    "mysql",
    "ai",
    "claude",
    "anthropic"
